<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads and installs the latest version of upterm from GitHub</string>
	<key>Identifier</key>
	<string>com.github.andrewvalentine.install.upterm</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>upterm</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.6.0</string>
    <key>ParentRecipe</key>
    <string>com.github.andrewvalentine.download.upterm</string>
    <key>Process</key>
    <array>
		<dict>
            <key>Processor</key>
            <string>InstallFromDMG</string>
            <key>Arguments</key>
            <dict>
				<key>dmg_path</key>
				<string>%pathname%</string>
                <key>items_to_copy</key>
				<array>
					<dict>
						<key>source_item</key>
						<string>%NAME%.app</string>
						<key>destination_path</key>
						<string>/Applications/</string>
					</dict>
				</array>
			</dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
    </array>
</dict>
</plist>
