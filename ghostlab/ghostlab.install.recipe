<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads and installs the latest release version of Ghostlab. A license is required from Vanamco for continued use.</string>
	<key>Identifier</key>
	<string>com.github.andrewvalentine.install.ghostlab</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Ghostlab2</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.2.9</string>
	<key>ParentRecipe</key>
	<string>com.github.andrewvalentine.download.ghostlab</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Processor</key>
			<string>InstallFromDMG</string>
			<key>Arguments</key>
			<dict>
				<key>dmg_path</key>
				<string>%pathname%</string>
				<key>items_to_copy</key>
				<array>
					<dict>
						<key>source_item</key>
						<string>Ghostlab2.app</string>
						<key>destination_path</key>
						<string>/Applications/</string>
					</dict>
				</array>
		</dict>
	</dict>
	</array>
</dict>
</plist>
